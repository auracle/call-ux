{
  "name": "call-ux",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:dev": "export NODE_ENV=\"development\"; npm run clean && babel src -d lib && gulp sass critical htmlmin && webpack",
    "build:pro": "export NODE_ENV=\"production\"; npm run clean && babel src -d lib && gulp sass critical htmlmin && webpack -p",
    "clean": "rimraf lib",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.js",
    "test": "export NODE_ENV=\"test\"; npm run test:unit",
    "test:unit": "mocha --compilers js:babel-core/register -r jsdom-global/register --require ./test/helper.jsx --recursive ./src",
    "version:patch": "npm --no-git-tag-version version patch",
    "version:minor": "npm --no-git-tag-version version minor",
    "version:major": "npm --no-git-tag-version version major"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-airbnb": "^2.2.3",
    "chai": "^4.1.0",
    "enzyme": "^3.2.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-react": "^7.4.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.8.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-sass": "^3.1.0",
    "jsdom": "^11.5.0",
    "jsdom-global": "^3.0.0",
    "mocha": "^4.0.0",
    "nock": "^9.0.13",
    "react-addons-test-utils": "^15.4.0",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "critical": "^1.0.0",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.0",
    "redux": "^3.0.4"
  }
}
